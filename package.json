{
  "name": "health-hub-backoffice-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "mock-server": "nodemon --watch mock mock/server.js",
    "dev:all": "concurrently --kill-others \"yarn mock-server\" \"yarn dev\" \"yarn codegen --watch 'graphql/**/*.(graphql|gql)'\" ",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint . --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc",
    "validate": "yarn check-types && yarn check-format && yarn lint && yarn build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@apollo/react-hooks": "4.0.0",
    "@graphql-tools/load-files": "6.2.7",
    "@graphql-tools/merge": "6.2.10",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "apollo-datasource-rest": "0.10.0",
    "apollo-server-express": "2.21.0",
    "cross-fetch": "3.0.6",
    "express": "4.17.1",
    "firebase": "8.2.9",
    "graphql": "15.5.0",
    "graphql-type-json": "0.3.2",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "next": "10.0.7",
    "nookies": "2.5.2",
    "notistack": "1.0.5",
    "react": "17.0.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dnd": "13.1.1",
    "react-dnd-html5-backend": "12.1.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.4",
    "react-text-mask": "5.4.3",
    "require-dir": "1.2.0",
    "styled-components": "5.2.1",
    "text-mask-addons": "3.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@testing-library/dom": "7.29.6",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.1",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-dom": "17.0.1",
    "@types/react-text-mask": "5.4.6",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "apollo-server-testing": "2.21.0",
    "babel-plugin-styled-components": "1.12.0",
    "concurrently": "6.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-testing-library": "3.10.1",
    "full-icu": "1.3.1",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.2.0",
    "jest-transform-graphql": "2.1.0",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "0.6.1",
    "json-server": "0.16.3",
    "lint-staged": "10.5.4",
    "mq-polyfill": "1.1.8",
    "nock": "13.0.10",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "typescript": "4.2.2",
    "webpack-graphql-loader": "1.0.2"
  }
}
