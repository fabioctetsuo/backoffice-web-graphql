{
  "name": "health-hub-backoffice-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "mock-server": "nodemon --watch mock mock/server.js",
    "dev:all": "concurrently --kill-others \"yarn mock-server\" \"yarn dev\" \"yarn codegen --watch 'graphql/**/*.(graphql|gql)'\" ",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint . --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc",
    "validate": "yarn check-types && yarn check-format && yarn lint && yarn build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/client": "3.2.4",
    "@apollo/react-hooks": "4.0.0",
    "@graphql-tools/load-files": "6.2.4",
    "@graphql-tools/merge": "6.2.4",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "apollo-datasource-rest": "0.9.4",
    "apollo-server-express": "2.18.2",
    "cross-fetch": "3.0.6",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "jest": "26.6.3",
    "lodash": "4.17.20",
    "next": "10.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "require-dir": "1.2.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@testing-library/dom": "7.26.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.8",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-plugin-styled-components": "1.11.1",
    "concurrently": "5.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-testing-library": "3.9.1",
    "full-icu": "1.3.1",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.1.2",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "0.6.1",
    "json-server": "0.16.2",
    "lint-staged": "10.4.0",
    "mq-polyfill": "1.1.8",
    "nodemon": "2.0.5",
    "prettier": "2.1.2",
    "typescript": "4.0.3",
    "webpack-graphql-loader": "1.0.2"
  }
}
